datasource db {
    provider  = "postgresql"
    url       = env("POSTGRES_PRISMA_URL")
    directUrl = env("POSTGRES_URL_NON_POOLING")
}

generator client {
    provider = "prisma-client-js"
}

model Person {
    namn         String
    val          String
    kanalid      String
    bestaellning Bestaellning @relation(references: [kanalid], fields: [kanalid])

    @@id(name: "testasd", [namn, kanalid])
}

model Bestaellning {
    kanalid        String   @id()
    restaurangnamn String
    webbadress     String
    kanalnamn      String
    personer       Person[]
}
