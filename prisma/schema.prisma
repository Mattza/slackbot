datasource db {
    provider  = "postgresql"
    url       = env("POSTGRES_PRISMA_URL")
    directUrl = env("POSTGRES_URL_NON_POOLING")
}

generator client {
    provider = "prisma-client-js"
}

model Person {
    name   String @id() @default(cuid())
    choose String
}

model Restaurang {
    name String @id() @default(cuid())
    url  String
}
